package com.hyemi;

import java.io.IOException;
import java.net.ServerSocket;
import java.net.Socket;

public class AcceptSocket extends Thread {
	
	public AcceptSocket(){
		Socket sock=null;
		ServerSocket server=null;
		try {
			server=new ServerSocket(8888);
			
			
			Reader.userInfo(sock);
		} catch (IOException e) {
			e.printStackTrace();
		}		
	}
	
}
